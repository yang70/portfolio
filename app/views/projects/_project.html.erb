<div class="col-md-4" id="<%= dom_id(project) %>">
  <div class="thing_container">
    <a href="/projects/<%= project.id %>">
      <% if project.image_url.present? %>
        <%= image_tag project.image_url , class: "thing_pic" %>
      <% else %>
        <img class="thing_pic" src="https://s3.amazonaws.com/matthewgyang/images/headshot_square.png" alt="headshot">
      <% end %>
      <div class='description'>
        <p class='description-content'><%= project.name %></p>
      </div>
      <div class='hover-description'>
        <p class='hover-title'><strong><%= project.name %></strong></p>
        <hr />
        <p><%= project.description %></p>
        <p><em><%= project.created_at.strftime("%m-%d-%Y") %></em></p>
      </div>
    </a>
  </div>
</div>
